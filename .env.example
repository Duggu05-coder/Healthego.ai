# AI Therapy Chatbot Environment Variables
# Copy this file to .env and fill in your actual values

# Required: Get your Gemini API key from https://ai.google.dev
GEMINI_API_KEY=your_gemini_api_key_here

# Optional: PostgreSQL database connection
# If not provided, app will use in-memory storage
DATABASE_URL=postgresql://username:password@localhost:5432/database_name

# Alternative PostgreSQL connection details
PGHOST=localhost
PGPORT=5432
PGUSER=your_username
PGPASSWORD=your_password
PGDATABASE=therapy_chatbot